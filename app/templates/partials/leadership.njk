<div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading">Leadership</h2>
                <h3 class="section-subheading text-muted">These people will keep you safe during the harrowing nights of our quarterly game of Assassins.</h3>
            </div>
        </div>
        <div class="row">
            {% for person in people %}
            <div class="col-sm-4">
                <div class="team-member">
                    <img class="mx-auto rounded-circle" src=
                    {% if person.placeholder == "N" %}"./img/lship/{{ person.picture }}"
                    {% else %}"./img/lship/placeholder.jpg"
                    {% endif %} 
                    alt="">
                    <h4>{{ person.name }}</h4>
                    <p class="text-muted">{{ person.position }}</p>
                    {# <ul class="list-inline social-buttons">
                        <li class="list-inline-item">
                            <a href="http://www.facebook.com/{{ person.facebook }}">
                        <i class="fa fa-facebook"></i>
                    </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="http://www.instagram.com/{{ person.instagram }}">
                        <i class="fa fa-instagram"></i>
                    </a>
                        </li>
                        <li class="list-inline-item">
                            <a href="http://twitter.com/{{ person.twitter }}">
                        <i class="fa fa-twitter"></i>
                    </a>
                        </li>
                    </ul> #}
                </div>
            </div>
            {% endfor %}
        </div>
        <div class="row">
            <div class="col-lg-8 mx-auto text-center">
                <p class="large text-muted"></p>
            </div>
        </div>